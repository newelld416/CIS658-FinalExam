{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@services": [
        "app/core/services"
      ],
      "@services/*": [
        "app/core/services/*"
      ],
      "@models": [
        "../models"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true
  }
}
